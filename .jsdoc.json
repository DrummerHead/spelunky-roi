{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["app/scripts"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  }
}
